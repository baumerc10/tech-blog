<h2>Blog Posts</h2>
<button type="submit" class="btn new-post-btn"><a class="link" href="/dashboard/new">Add a Post</a></button>
<section>
    {{#if posts.length}}
    {{#each posts as |post|}}
    {{> dashboard-postinfo post}}
    {{/each}}
    {{/if}}
</section>
<script src="./js/add-post.js"></script>